{
  "name": "remix-cloudflare-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "check": "biome check && knip",
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "wrangler dev",
    "deploy": "wrangler deploy",
    "format": "biome format . --write",
    "lint": "biome lint",
    "lint:fix": "biome check . --write",
    "test": "vitest run --browser.headless",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --browser.headless",
    "typecheck": "bun run typegen && tsc --noEmit",
    "typegen": "wrangler types && react-router typegen",
    "lighthouse": "wrangler dev --port 8787 & while ! nc -z 127.0.0.1 8787; do sleep 1; done && unlighthouse-ci --site http://127.0.0.1:8787"
  },
  "keywords": [],
  "author": "VHS <vhsdev@tutanota.com>",
  "license": "OBSD",
  "description": "Versatile remix starter template for Cloudflare Pages",
  "dependencies": {
    "@react-router/cloudflare": "^7.1.1",
    "isbot": "^4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.1",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/workers-types": "^4.20241205.0",
    "@react-router/dev": "^7.1.1",
    "@react-router/fs-routes": "^7.1.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@unlighthouse/cli": "^0.15.0",
    "@unlighthouse/core": "^0.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^2.1.8",
    "@vitest/coverage-v8": "2.1.8",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "cssnano-preset-advanced": "^7.0.6",
    "happy-dom": "^15.11.7",
    "knip": "^5.41.1",
    "lucide-react": "^0.469.0",
    "playwright": "^1.49.1",
    "postcss": "^8.4.49",
    "react-aria-components": "^1.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "wrangler": "^3.95.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=22.11.0",
    "bun": ">=1.1.36"
  }
}
